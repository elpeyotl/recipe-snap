# ============================================
# Recipe Snap Environment Variables
# ============================================
# Copy this file to .env and fill in the values
# NEVER commit .env to git!

# ============================================
# GEMINI API
# ============================================
# Get your API key from https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# ============================================
# APP CONFIG (Optional)
# ============================================
# Unlock code for unlimited access (for testing/promos)
VITE_UNLOCK_CODE=

# Set to 'false' to disable snap limit (unlimited snaps for testing)
VITE_LIMIT_SNAPS=true
